    "@types/express": "^4.0.30",
    "@types/node": "^6.0.32",
    "@types/serve-static": "^1.7.28",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.1.1",
    "core-js": "^2.4.0",
    "cpy-cli": "^1.0.1",
    "http-server": "^0.9.0",
    "js-beautify": "^1.6.3",
    "js.clone": "0.0.3",
    "json-loader": "^0.5.4",
    "nodemon": "^1.9.2",
    "pm2": "^1.1.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "string-replace-loader": "github:gdi2290/string-replace-loader",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0-dev.0",
    "typescript": "^2.0.0-beta",
    "webpack": "2.1.0-beta.20"
  },
  "dependencies": {
    "@angular/common": "github:angular/common-builds",
